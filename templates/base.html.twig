<!DOCTYPE html>
<html>
    <head>
        {% block head %}{% endblock %}
        <meta charset="UTF-8">
        <title>
            {% block title %}Welcome!
            {% endblock %}
        </title>
        {% block meta %}{% endblock %}
        <link href="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.2.1/flowbite.min.css" rel="stylesheet"/> {% block stylesheets %}{% endblock %}
        {{ encore_entry_link_tags('app') }}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        {% block importmap %}
            {{ importmap('app') }}
        {% endblock %}
    </head>
    <body>

        {% include '/components/menu.html.twig' %}
        {% block body %}{% endblock %}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.2.1/flowbite.min.js"></script>
        <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>


    </body>
</html>
